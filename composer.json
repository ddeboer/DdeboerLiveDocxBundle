{
    "name": "ddeboer/livedocx-bundle",
    "type": "symfony-bundle",
    "description": "Include the Zend Framework's LiveDocx client in your Symfony2 project",
    "keywords": [ "doc", "pdf", "mailmerge" ],
    "homepage": "https://github.com/ddeboer/DdeboerLiveDocxBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "repositories": [
		{
	        "type": "package", 
	        "package": {
	            "name": "zend/zf2",
	            "version": "dev-master",
	            "source": {
	                "type": "git",
	                "url": "http://github.com/zendframework/zf2.git",
	                "reference": "master"
	            },
	            "autoload": {
	                "psr-0": { "Zend\\": "library/" }
	            }
	        }
	    }
	],
    "require": {
        "zend/zf2": ">=2.0.*-dev"
    },
    "autoload": {
        "psr-0": { "Ddeboer\\LiveDocxBundle": "" }
    },
    "target-dir": "Ddeboer/LiveDocxBundle"    
}